# =============================================================================
# Local Development Defaults
# =============================================================================
# This file contains default values for local development with LocalStack.
# It is loaded after .env and will not override values already set.
# Do not commit secrets to this file.

# Chain ID (1 = Ethereum Mainnet)
CHAIN_ID=1

# SNS FIFO Topic ARN (single topic for all event types)
AWS_SNS_TOPIC_ARN=arn:aws:sns:us-east-1:000000000000:stl-ethereum-blocks.fifo

# LocalStack (Local AWS SNS/SQS)
AWS_SNS_ENDPOINT=http://localhost:4566
AWS_ACCESS_KEY_ID=test
AWS_SECRET_ACCESS_KEY=test
AWS_REGION=us-east-1